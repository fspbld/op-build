From 439c21e2704dac2d7cd72eafd08ab52cf298dfa9 Mon Sep 17 00:00:00 2001
From: Bill Hoffa <wghoffa@us.ibm.com>
Date: Thu, 28 Sep 2017 16:27:54 -0500
Subject: [PATCH] XML Updates for some Attrs   - Change SUPPORTED_STOP_STATES
 to 0xEC100000   - Set SySTEM_VDM_DISABLE to 0x0   - Set SYSTEM_RESCLK_DISABLE
 to 0x0

---
 witherspoon.xml | 10 +++++++++-
 1 file changed, 9 insertions(+), 1 deletion(-)

diff --git a/witherspoon.xml b/witherspoon.xml
index 55cdf06..3dce1f0 100644
--- a/witherspoon.xml
+++ b/witherspoon.xml
@@ -17237,12 +17237,20 @@
 	</attribute>
 	<attribute>
 		<id>SUPPORTED_STOP_STATES</id>
-		<default>0x80000000</default>
+		<default>0xEC100000</default>
 	</attribute>
 	<attribute>
 		<id>SUPPORTS_DYNAMIC_MEM_VOLT</id>
 		<default>0</default>
 	</attribute>
+        <attribute>
+                <id>SYSTEM_VDM_DISABLE</id>
+                <default>0x0</default>
+        </attribute>
+        <attribute>
+                <id>SYSTEM_RESCLK_DISABLE</id>
+                <default>0x0</default>
+        </attribute>
 	<attribute>
 		<id>SYSTEM_CORE_PERIODIC_QUIESCE_DISABLE</id>
 		<default>0x00</default>
